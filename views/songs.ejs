<%- include('./_header'); -%>

<form id="selection" action="/recommend" method="POST">
    <label><h1 name="artist" value="<%=artist%>">Select Songs by <%=artist%>:</h2></label>
    <label><input name="artist" type="hidden" value="<%=artist%>"><br></label>
    <% data.forEach(function(d) { %>
        <div class="checkbox">
            <label><input name="music" type="checkbox" value="<%=d.name%>"><%= d.name %><br></label>
        </div>
        <!---<a href="<%= d.href %>"> + <%= d.name %> + </a><br> --->
    <% }) %>
    <input type="submit" value="Generate Playlist">
</form>